{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "downlevelIteration": true,
    "module": "esnext",
    "esModuleInterop": true,
    "jsx": "react",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "lib": [
      "esnext.full",
      "dom"
    ]
  },
  "include": [
    "src/**/*",
    "../../typings/**/*.d.ts"
  ]
}
