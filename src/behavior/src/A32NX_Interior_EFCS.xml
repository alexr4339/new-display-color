<ModelBehaviors>
    <!-- ELAC switches -->
    <Template Name="FBW_AIRBUS_Push_ELAC_Template">
        <UseTemplate Name="FBW_Push_Toggle">
            <Switch Param="ID">
                <Case Value="1">
                    <NODE_ID>PUSH_OVHD_FLTCTL_ELAC</NODE_ID>
                </Case>
                <Default>
                    <NODE_ID>PUSH_OVHD_FLTCTL_ELAC#ID#</NODE_ID>
                </Default>
            </Switch>
            <LEFT_SINGLE_CODE>(L:A32NX_FBW_ELAC_SWITCH:#ID#, Bool) ! (&gt;L:A32NX_FBW_ELAC_SWITCH:#ID#, Bool)</LEFT_SINGLE_CODE>
            <DOWN_CODE>(L:A32NX_FBW_ELAC_SWITCH:#ID#, Bool)</DOWN_CODE>

            <SEQ1_CODE>(L:A32NX_FBW_ELAC_FAILED:#ID#, Bool) (L:A32NX_FBW_ELAC_SWITCH:#ID#, Bool) and</SEQ1_CODE>
            <SEQ2_CODE>(L:A32NX_FBW_ELAC_SWITCH:#ID#, Bool) !</SEQ2_CODE>
            <SEQ2_EMISSIVE_DRIVES_VISIBILITY>False</SEQ2_EMISSIVE_DRIVES_VISIBILITY>

            <TOOLTIPID>%((L:A32NX_FBW_ELAC_SWITCH:#ID#, Bool) !)%{if}TT:COCKPIT.TOOLTIPS.ELAC#ID#_TURN_ON%{else}TT:COCKPIT.TOOLTIPS.ELAC#ID#_TURN_OFF%{end}</TOOLTIPID>
        </UseTemplate>
    </Template>

